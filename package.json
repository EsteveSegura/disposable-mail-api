{
  "name": "disposable-mail-api",
  "version": "0.0.7",
  "description": "Generate multiple disposable mails",
  "main": "./src/index.js",
  "bin": {
    "disposable-mail-api": "./bin/cli.js"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --testPathPattern=/test/ --passWithNoTests",
    "lint": "eslint .",
    "examples": "node ./example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EsteveSegura/disposable-mail-api.git"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>",
      "<rootDir>/test/"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "keywords": [
    "mail",
    "disposable",
    "temporal",
    "mail",
    "testing",
    "mails",
    "email",
    "testing",
    "emails"
  ],
  "author": "Esteve 'GiR' Segura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EsteveSegura/disposable-mail-api/issues"
  },
  "homepage": "https://github.com/EsteveSegura/disposable-mail-api#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "figlet": "^1.5.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.4.2"
  }
}
